function [output] = cobs2(input)
%COBS2 Summary of this function goes here
%   Detailed explanation goes here

input(end+1) = 0
len = length(input)
output = zeros(0.)

output(1)= uint8(0);
lastIndex = 1;

for i = 1:length(input)
    if input(i) == 0
        output(lastIndex) = (i + 1) - lastIndex;
        lastIndex = i + 1;
    else
        output(i+1) = input(i);
    end
end        

end



